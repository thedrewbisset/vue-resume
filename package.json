{
  "name": "vue-resume",
  "version": "1.0.0",
  "description": "A simplified and lean version of the best-resume-ever project: https://github.com/salmonelli/best-resume-ever",
  "author": "drewbisset",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thedrewbisset/vue-resume.git"
  },
  "scripts": {
    "docs": "node build/build.js",
    "docs:serve": "cd docs/ && ws --port 8080 --rewrite '/vue-resume/* -> /$1'",
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "test:deleteFiles": "node test/scripts/deleteFiles.js",
    "test:export": "SCRIPT=export npm run test:deleteFiles && npm run export && mocha test/scripts/export.js",
    "test:docs": "npm run docs && npm run docs:serve",
    "test": "npm run test:export && npm run test:docs",
    "export": "concurrently \"npm run dev\" \"npm run pdf\" --success first --kill-others --raw",
    "lint": "eslint --ext .js,.vue src scripts",
    "lint:fix": "eslint --ext .js,.vue src scripts --fix"
  },
  "dependencies": {
    "font-awesome": "4.7.0",
    "js-yaml": "3.13.1",
    "material-design-icons": "3.0.1",
    "material-icons": "0.2.3",
    "roboto-fontface": "0.10.0",
    "vue": "2.6.11",
    "vue-router": "3.1.6"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "autoprefixer": "9.7.6",
    "babel-loader": "8.1.0",
    "chalk": "4.0.0",
    "check-node-version": "4.0.3",
    "concurrently": "5.1.0",
    "connect-history-api-fallback": "1.6.0",
    "copy-webpack-plugin": "^5.1.1",
    "cpx-fixed": "1.6.0",
    "css-loader": "3.5.2",
    "escope": "3.6.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.0",
    "eslint-plugin-html": "6.0.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.17.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "6.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "4.2.0",
    "http": "0.0.1-security",
    "http-proxy-middleware": "1.0.3",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "local-web-server": "4.0.0",
    "@sinonjs/fake-timers": "6.0.0",
    "mocha": "7.1.1",
    "mv": "2.1.1",
    "node": "13.11.0",
    "open": "6.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "ora": "4.0.4",
    "pdf-image": "2.0.0",
    "postcss": "7.0.27",
    "postcss-cssnext": "3.1.0",
    "puppeteer": "3.0.1",
    "rename": "1.0.4",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "semver": "7.3.2",
    "shelljs": "0.8.3",
    "url-exists": "1.0.3",
